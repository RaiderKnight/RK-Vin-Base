WHAT: none
WHERE: home
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        If isModEnabled(nn_VB_HomeExpander)
            "Which room should I check?"
            0::"Go to the bathroom"
            1::"Go to the bedroom"
            2::"Go to the bedroom 2"
            3::"Go to the bedroom 3"
            4::"Go to the homeoffice"
            5::"Go to the kitchen"
            6::"Go to the livingroom"
            99::"I changed my mind!"
            
            If 0
                followUp(nn_re_go_to_bathroom)
            ElseIf 1
                followUp(nn_re_go_to_bedroom)
            ElseIf 2
                followUp(nn_re_go_to_bedroom2)
            ElseIf 3
                followUp(nn_re_go_to_bedroom3)
            ElseIf 4
                followUp(nn_re_go_to_homeoffice)
            ElseIf 5
                followUp(nn_re_go_to_kitchen)
            ElseIf 6
                followUp(nn_re_go_to_livroom) 
            EndIf
        Else
            "You need to enable the mod Home Expander..."
        EndIf
    SceneEnd()