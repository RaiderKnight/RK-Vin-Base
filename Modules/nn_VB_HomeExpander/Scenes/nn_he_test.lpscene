WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        If false
            If WHEN > 22 || WHEN < 6
                "NIGHT"
            ElseIf When >= 6 && WHEN <= 8
                "EARLY MORNING"
            ElseIf When > 8 && WHEN <= 12
                "MORNING"
            ElseIf When > 12 && WHEN <= 13
                "NOON"
            ElseIf When > 13 && WHEN <= 18
                "AFTERNOON"
            ElseIf When > 18 && WHEN <= 22
                "EVENING"
            EndIf
        EndIf
        
        Type = "work"
        If isOpen(type)
            "Open"
        Else
            "Closed"
        EndIf
        
        "DONE!" 
    SceneEnd()
