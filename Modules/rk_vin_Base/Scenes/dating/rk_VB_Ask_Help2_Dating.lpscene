WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()

        Dating = getSpecific(Dating)
        "What kind of help I need from <Dating.name>?"
        0:: cooking < Random(0, 60) && Dating:cooking > 70 ::"Ask for help with cooking"
        1:: dance < Random(0, 60) && Dating:dance > 70 ::"Ask for help with dance"
        2:: music < Random(0, 60) && Dating:music > 70 ::"Ask for help with music"
        3:: interpersonal < Random(0, 60) && Dating:interpersonal > 70 ::"Ask for help with my sociability"
        4:: intelligence < Random(0, 60) && Dating:intelligence > 70 ::"Ask for help with reading books"
        5:: perversion < Random(0, 60) && Dating:perversion > 70 ::"Ask for help to learn more sex positions"
        6:: fitness < Random(0, 60) && Dating:fitness > 70 ::"Ask for help with training"
        7:: money < 0 ::"Ask for help financially"
        99::"I changed my mind"

        If 0
        
            followUp(dating_cooks) //
        
        ElseIf 1
        
            followUp(dating_dances) //
            
        ElseIf 2
        
            followUp(dating_serenades) //
        
        ElseIf 3
        
            followUp(sociable_dating) //
            
        ElseIf 4
        
            followUp(intelligent_dating) //

        ElseIf 5
        
            followUp(perverted_dating) //
            
        ElseIf 6
        
            followUp(fit_dating) //
            
        ElseIf 7
        
            followUp(money_from_dating) //
            
        Else
            
            "Although <Dating.name> promised to help me, <Dating.he_or_she> ended up being too busy to spend time with me."

        EndIf

    SceneEnd() // Scene improved by Raddeck