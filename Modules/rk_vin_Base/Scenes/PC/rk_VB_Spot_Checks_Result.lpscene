WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
    
        Hunger = Player.getActorVar(rk_VB_Hunger)
        HungerEnable = rk_VB_EnableHunger.getGlobal()
        DieHungerEnable = rk_VB_DieHungerEnable.getGlobal()
        
        If HungerEnable > 0 && Hunger <= 0
            
            "GAME OVER! You just die of starvation!"
            0:: "Load last save"
            1:: "Revive my character (cheat)"
    
            If 0
            
                sceneEndLoadLastSave()
                
            Else
            
                Player.modifyActorVar(rk_VB_Hunger, 15)
                sceneEnd()
                
            Endif
            rk_VB_DieHungerEnable.setGlobal(0)
            
        EndIf
        
    SceneEnd()