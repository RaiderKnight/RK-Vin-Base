WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()

        SettingChoiceSwitch = rk_VB_SettingChoiceSwitchPC.getGlobal()
        SettingChoiceSwitchS = rk_VB_SettingChoiceSwitchS.getGlobal()
        SettingChoiceSwitchFood = rk_VB_SettingChoiceSwitchFood.getGlobal()
        
        
        "What I want to do?"
        0::"Set Inventories"
        1::"Place LifePlay items in inventory"
        99::"Go back to game!"
        
        If 0
        
            "Which item you want to set inventory?"
            3::"Set food inventory"
            5::"Set cigars inventory"
            20::"Set Weed inventory"
            50::"Set computers inventory"
            51::"Set mobile phones inventory"
            99::"Go back to game!"
            
            If 3
                
                "Which item you want to set inventory?"
                3::"Set Bakery inventory"
                5::"Set Candy inventory"
                7::"Set Ice cream desserts inventory"
                8::"Set Meat meals inventory"
                10::"Set Popcorn inventory"
                13::"Set Seafood inventory"
                15::"Set Vegan meals inventory"
                99::"Go back to game!"
                
                If 3
                    rk_VB_SettingChoiceSwitchFood.setGlobal(1)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 5
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(2)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 7
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(3)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 8
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(4)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 10
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(5)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 13
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(6)
                    followUp(rk_VB_HandSwitchInv_Food)
                    
                ElseIf 15
                
                    rk_VB_SettingChoiceSwitchFood.setGlobal(7)
                    followUp(rk_VB_HandSwitchInv_Food)
                
                EndIf
            
            ElseIf 5
                
                rk_VB_SettingChoiceSwitchS.setGlobal(1)
                followUp(rk_VB_HandSwitchInv_Smoke)
            
            ElseIf 20
                
                rk_VB_SettingChoiceSwitchS.setGlobal(2)
                followUp(rk_VB_HandSwitchInv_Smoke)
            
            ElseIf 50
                
                rk_VB_SettingChoiceSwitchPC.setGlobal(1)
                followUp(rk_VB_HandSwitchInv_PC)
            
            ElseIf 51
                
                rk_VB_SettingChoiceSwitchPC.setGlobal(2)
                followUp(rk_VB_HandSwitchInv_MPho)
                
            EndIf
        
        ElseIf 1
        
            "Click on the item you want to store!"
            90:: stock_musicalinstrument > 0 ::"Musical instrument (<stock_musicalinstrument>)"
            99::"Go back to game!"
            
            
            If 90
            
                followUp(rk_VB_HandSwitchInv_Musik)
            
            EndIf
            
        EndIf
        
    SceneEnd()