WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
    
        clearGetList()
        
        hasLivingroom = RE_has_lvingroom.getGlobal()
        hasKitchen = RE_has_kitchen.getGlobal()
        hasOfficeroom = RE_has_officeroom.getGlobal()
        hasSecBedroom = RE_has_secondbedroom.getGlobal()
        hasThirdBedroom = RE_has_thirdbedroom.getGlobal()
        
        stock_musicalinstrument_LR = Player.getActorVar(stock_musicalinstrument_LR)
        stock_musicalinstrument_MB = Player.getActorVar(stock_musicalinstrument_MB)
        stock_musicalinstrument_O = Player.getActorVar(stock_musicalinstrument_O)
        stock_musicalinstrument_SB = Player.getActorVar(stock_musicalinstrument_SB)
        stock_musicalinstrument_SR = Player.getActorVar(stock_musicalinstrument_SR)
        stock_musicalinstrument_TB = Player.getActorVar(stock_musicalinstrument_TB)
        TotalInstMusik = stock_musicalinstrument_LR + stock_musicalinstrument_MB + stock_musicalinstrument_O + stock_musicalinstrument_SB + stock_musicalinstrument_SR + stock_musicalinstrument_TB
        
        IBuyIMusick = Player.getActorVar(rk_VB_IBuyIMusick)
        
        "Where I will store this musical instrument?"
        2::isModEnabled(nn_HomeExpander) && hasLivingroom ::"Inside my living room"
        3::"Inside my bedroom"
        4::isModEnabled(nn_HomeExpander) && hasOfficeroom::"Inside my office room"
        5::isModEnabled(nn_HomeExpander) && hasSecBedroom::"Inside my second bedroom"
        6::isModEnabled(rk_MoneyMaker) || isModEnabled(rk_Art_items_Shop)::"Inside my storage room"
        7::isModEnabled(nn_HomeExpander) && hasThirdBedroom::"Inside my third bedroom"
                
        If 2
                
            "Added to your living room!"
            Player.modifyActorVar(stock_musicalinstrument_LR, 1)
            stock_musicalinstrument -= 1
                    
        ElseIf 3
                
            "Added to your bedroom!"
            Player.modifyActorVar(stock_musicalinstrument_MB, 1)
            stock_musicalinstrument -= 1
                    
        ElseIf 4
                
            "Added to your office room!"
            Player.modifyActorVar(stock_musicalinstrument_O, 1)
            stock_musicalinstrument -= 1
                    
        ElseIf 5
                
            "Added to your second bedroom!"
            Player.modifyActorVar(stock_musicalinstrument_SB, 1)
            stock_musicalinstrument -= 1
                    
        ElseIf 6
                
            "Added to your storage room!"
            Player.modifyActorVar(stock_musicalinstrument_SR, 1)
            stock_musicalinstrument -= 1
                    
        ElseIf 7
                
            "Added to your third bedroom!"
            Player.modifyActorVar(stock_musicalinstrument_TB, 1)
            stock_musicalinstrument -= 1
        
        ElseIf 8
                
            "Added to my pocket!"
            Player.modifyActorVar(stock_musicalinstrument_P, 1)
            stock_musicalinstrument -= 1

        EndIf
        
    SceneEnd()