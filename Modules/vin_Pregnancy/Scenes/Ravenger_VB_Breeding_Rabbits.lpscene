WHAT: all 
WHERE: all
WHEN: 0 - 20
WHO: none
OTHER: !IsWithCompanion() && !Player.isFemale()

SceneStart()
    Breeder = getPerson(tag_Breeder)
    
    If Breeder.isValid() && Breeder.WantsBabies(true) && !Breeder.isMale() && Breeder.isOnPeriod() && !Breeder.isPregnant()
    
            Hunger = Player.getActorVar(rk_VB_Hunger)
            HungerEnable = rk_VB_EnableHunger.getGlobal()
            UrgeToPeeEnable = rk_VB_UrgeToPeeEnable.getGlobal()
            UrgeToPee = Player.getActorVar(rk_VB_UrgeToPee)
            setBackground(home)
            Breeder.dress()
            Breeder.show()
            Breeder():: "I want you to fuck me till I'm pregnant."
            0:: "Let get started then."
            1:: "Sorry not today."
        
            If 0
                Breeder.setActorVar(tag_Breeder, 1)
                Breeder.Strip()
                Player.Strip()
                Player(flirty):: "Let get started then."
                Breeder:perversion += 45
                Breeder:likes_vaginal += 100
                Breeder:attractiontoplayer += 10
                Filter(vaginal)
                Sex(Player, Breeder)
                If HungerEnable > 0
                    Player.modifyActorVar(rk_VB_Hunger, -2)
                EndIf
                Player(flirty):: "Wow, that was fun."
                If !Breeder.isPregnant()
                    Breeder():: "We are not done yet..."
                    Breeder:fertility_npc += 5
                    energy -= 15
                    Filter(vaginal)
                    SexNoAffair(Player, Breeder)
                    If HungerEnable > 0
                        Player.modifyActorVar(rk_VB_Hunger, -2)
                    EndIf
                    Player(tired):: "Done?"
                    If !Breeder.isPregnant()
                        Breeder():: "No keep on cuming"
                        Breeder:fertility_npc += 10
                        energy -= 15
                        Filter(vaginal)
                        SexNoAffair(Player, Breeder)
                        If HungerEnable > 0
                            Player.modifyActorVar(rk_VB_Hunger, -2)
                        EndIf
                        Breeder:attractiontoplayer += Random(1, 30)
                        Player(tired):: "Good lord... I'm empty..."
                    Endif
                Endif           
            Else
                "Sorry not today."
            Endif
            If Breeder.isPregnant()
                Breeder.setActorVar(tag_Breeder, -1)
                "<Breeder.name> got Pregnant."
            Endif

        timeoutActor(400, Ravenger_VB_Breeding_Rabbits, Breeder)
        timeout(200, Ravenger_VB_Breeding_Rabbits) 
    Endif
SceneEnd() // Improved by Raddeck