WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        stock_viagra = Player.getActorVar(rk_VB_stock_viagra)
        If stock_viagra <= 0
            "I'm out of viagra pills! I have to go to the pharmacy to get more..."
        Else
            "I gave one viagra pill to <Actor.him_or_her>!"
            Player.modifyActorVar(rk_VB_stock_viagra, -1)
            Actor:rapportwithplayer += 2
            Actor:arousal += 50
            Actor:energy += 50
        EndIf
    SceneEnd()