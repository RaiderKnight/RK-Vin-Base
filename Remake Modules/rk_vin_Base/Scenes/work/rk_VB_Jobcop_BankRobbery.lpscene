                            ElseIf eventBank == 2 && TMaForceS > random(0, 50)
                                "Great, we managed to kill one of them!"
                                MaTuerV += 1
                            ElseIf eventBank == 3 && TMaForce > random(0, 50)
                                "Well, we managed to captured one of the robbers!"
                                capturerR += 1
                            ElseIf eventBank == 4 && TForcNpc > random(0, 100)
                                "Damn, they managed to kill one of the hostages!"
                                OtagesTuer += 1
                            ElseIf eventBank == 5 && TMaForce > random(0, 50)
                                "Well, we managed to save one of the hostages!"
                                capturer += 1
                            EndIf
                        EndIf
                    EndWhile
                EndIf
                Rounder += 1
            EndWhile
            If JobFait
                If OtagesTuer == 0 && NVoleursT == voleurs
                    Bnus1 = NVoleursT*100
                    Bnus2 = Notages*1000
                    money = Bnus1 + Bnus2
                Else
                    Bnus1 = NVoleursT*100
                    Bnus2 = Notages*1000
                    money = Bnus1 + Bnus2
                EndIf
                mood -= random(5, 10)
                energy -= random(15, 30)
                If HungerEnable > 0
                    Player.modifyActorVar(rk_VB_Hunger, -10)
                EndIf
                If Notages > 0
                    Player.modifyActorVar(rk_VB_PoliceXP, Notages)
                    jobperformance += Notages/2
                    jobexperience += Notages/2
                Else
                    Player.modifyActorVar(rk_VB_PoliceXP, 0.25)
                    jobperformance += 0.25
                    jobexperience += 0.25
                EndIf
                "I made my report to my supervisors, and not surprisingly they were very impressed with my performance on this call!"
            EndIf
            If nogoodP
                "I made my report to my supervisors, and not surprisingly they were very mot impressed with my performance on this call!"
                mood -= random(5, 10)
                energy -= random(15, 30)
                If HungerEnable > 0
                    Player.modifyActorVar(rk_VB_Hunger, -10)
                EndIf
                Player.modifyActorVar(rk_VB_PoliceXP, 0.25)
                jobperformance += 0.25
                jobexperience += 0.25
            EndIf
            passTime(1, 3)
            Player.modifyActorVar(rk_VB_gfaitmajobcop, 1)
        EndIf
    SceneEnd()