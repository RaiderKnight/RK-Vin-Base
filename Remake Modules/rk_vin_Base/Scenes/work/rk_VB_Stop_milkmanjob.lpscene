WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        Hunger = Player.getActorVar(rk_VB_Hunger)
        HungerEnable = rk_VB_EnableHunger.getGlobal()
        UrgeToPeeEnable = rk_VB_UrgeToPeeEnable.getGlobal()
        UrgeToPee = Player.getActorVar(rk_VB_UrgeToPee)
        Job_MilkMan = rk_VB_Job_MilkMan.getGlobal()
        MilkManSalary = Player.getActorVar(rk_VB_MilkManSalary)
        MilkManXP = Player.getActorVar(rk_VB_MilkManXP)
        "Are you sure you want to stop being a milkman?"
        0::"Yes"
        1::"I changed my mind"
        
        If 0
            Player.setActorVar(rk_VB_MilkManSalary, 0)
            Player.setActorVar(rk_VB_MilkManXP, 0)
            rk_VB_Job_MilkMan.setGlobal(0)
            "All done!"
        Else
            "Well, I have nothing but this job to earn money! I'll think about it again!"
        EndIf
    SceneEnd()