WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none


        
    If isModEnabled(rk_vin_Base)
        clearGetList()
        FratA = getPerson(rk_VB_tag_Student)
        FratB = getSpecific(Fraternity)
        FratKitch = rk_VB_Fratkitchen.getGlobal()
        If FratKitch <= 0
            rk_VB_Fratkitchen.setGlobal(1)
        EndIf
        FratA.hide()
        FratB.hide()
        If isModEnabled(nn_VB_HomeExpander)
            setBackground3D(Modules/nn_VB_HomeExpander/Rooms/re_kitchen.lpworld)
        Else
            setBackground3D(Modules/vin_Base/World/kitchen___vin01.lpworld)
        EndIf
        Actor = getCreature()
        If Actor.isValid() && Actor.isCreatureType(Dog)
            chance = random(0,6)
            If chance>=3
                Actor.show()
            Else
                Actor.hide()
            EndIf
        EndIf
        Player.show(7,40,0)  
        cameraFocus(Player)
        If FratA.isValid() 
            If random(0,10) >= 7 && [[WHEN>=6 && WHEN<=10] || [WHEN>=16 && WHEN<=23]]
                "FratA..."
                FratA.dress()
                FratA.show()
            Else
                FratA.show()
                FratA.setNoRedress(true)
                If FratB.isValid() && FratB.isInterestedIn(FratA)
                    FratB.show()
                    FratB.setNoRedress(true)
                    Sex(FratA, FratB)
                EndIf
            EndIf
        EndIf
        If FratB.isValid() 
            If random(0,10) >= 7 && [[WHEN>=6 && WHEN<=10] || [WHEN>=16 && WHEN<=23]]
                "FratB..."
                FratB.dress()
                FratB.show()
            Else
                FratB.show()
                FratB.setNoRedress(true)
                If FratA.isValid() && FratA.isInterestedIn(FratA)
                    FratA.show()
                    FratA.setNoRedress(true)
                    Sex(FratB, FratA)
                EndIf
            EndIf
        EndIf
    EndIf
