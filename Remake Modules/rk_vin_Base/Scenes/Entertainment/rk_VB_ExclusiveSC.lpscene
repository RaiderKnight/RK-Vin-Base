WHAT: none
WHERE: none
WHEN: 0-24
WHO: none
OTHER:none

    SceneStart()
        // Idea provided by Olli66 - Story/coding created by Raddeck
        SCsetPrice = Player.getActorVar(rk_VB_SCsetPrice)
        SCsetDate = Player.getActorVar(rk_VB_SCsetDate)
        SCsetDateT = Player.getActorVar(rk_VB_SCsetDateTemp)
        SCsetDateGo = rk_VB_SCsetDateGo.getGlobal()
        Hunger = Player.getActorVar(rk_VB_Hunger)
        HungerEnable = rk_VB_EnableHunger.getGlobal()
        UrgeToPeeEnable = rk_VB_UrgeToPeeEnable.getGlobal()
        UrgeToPee = Player.getActorVar(rk_VB_UrgeToPee)
        setESwingerClub = rk_VB_setESwingerClub.getGlobal()
        inscrip = random(1000, 2000)
        inscripl = inscrip.convertToLocalCurrency()
        SCsetPricel = SCsetPrice.convertToLocalCurrency()
        Dating = getSpecific(Dating)
        If Dating.isValid()
            Dating.hide()
        EndIf
        entreSalle = false
        entre = false
        Mutter = Player:age
        Mot = Player:age*365
        Mot = Mot.round()
        Mot1 = Mot + 30
        Mot1 = Mot1.round()
        Mot2 = SCsetDateT - SCsetDate
        beinito = false
        If setESwingerClub >= 1
            If setESwingerClub == 1
                setRaceGenerated(Arab)
            ElseIf setESwingerClub == 2
                setRaceGenerated(Asian)
            ElseIf setESwingerClub == 3
                setRaceGenerated(Black)
            ElseIf setESwingerClub == 4
                setRaceGenerated(Latin)
            ElseIf setESwingerClub == 5
                setRaceGenerated(Indian)
            ElseIf setESwingerClub == 6
                setRaceGenerated(White)
            EndIf
            DoorMan = generatePersonTemporary()
            While !DoorMan.isInterestedIn(Player)
                DoorMan = generatePersonTemporary()
            EndWhile
            If DoorMan.isEthnic(black) && DoorMan.isMale()
                DoorMan.blendPreset(strt_black_m)
            ElseIf DoorMan.isEthnic(black) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_black_f)
            ElseIf DoorMan.isEthnic(indian) && DoorMan.isMale()
                DoorMan.blendPreset(strt_indian_m)
            ElseIf DoorMan.isEthnic(indian) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_indian_f)
            ElseIf DoorMan.isEthnic(arab) && DoorMan.isMale()
                DoorMan.blendPreset(strt_arab_m)
            ElseIf DoorMan.isEthnic(arab) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_arab_f)
            ElseIf DoorMan.isEthnic(latin) && DoorMan.isMale()
                DoorMan.blendPreset(strt_latin_m)
            ElseIf DoorMan.isEthnic(latin) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_latin_f)
            ElseIf DoorMan.isEthnic(asian) && DoorMan.isMale()
                DoorMan.blendPreset(strt_asian_m)
            ElseIf DoorMan.isEthnic(asian) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_asian_f)
            ElseIf DoorMan.isEthnic(white) && DoorMan.isMale()
                DoorMan.blendPreset(strt_white_m)
            ElseIf DoorMan.isEthnic(white) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_white_f)
            EndIf
            DoorMan.blendPreset(bodybuilder)
            DoorMan:age => random(25, 55)
            DoorMan:muscle => random(35, 75)
            DoorMan:fitness => random(15, 75)
            DoorMan:martial => random(5, 75)
            DoorMan.randomizeFace()
            DoorMan.randomizeHairs()
            DoorMan.dress()
            DoorMan.hide()
        EndIf
        If setESwingerClub == 0
            EthChoix = 0
            If Player.isEthnic(arab)
                EthChoix = 1
            ElseIf Player.isEthnic(asian)
                EthChoix = 2
            ElseIf Player.isEthnic(black)
                EthChoix = 3
            ElseIf Player.isEthnic(indian)
                EthChoix = 4
            ElseIf Player.isEthnic(Latin)
                EthChoix = 5
            ElseIf Player.isEthnic(White)
                EthChoix = 6
            Else
                "Something wrong happen! Report this bug to Raddeck"
            EndIf
            //EthChoix = random(1, 6)
            //EthChoix = EthChoix.round()
            If EthChoix == 1
                ETHtxt = "arab"
                setRaceGenerated(arab)
            ElseIf EthChoix == 2
                ETHtxt = "asian"
                setRaceGenerated(asian)
            ElseIf EthChoix == 3
                ETHtxt = "black"
                setRaceGenerated(black)
            ElseIf EthChoix == 4
                ETHtxt = "latin"
                setRaceGenerated(latin)
            ElseIf EthChoix == 5
                ETHtxt = "indian"
                setRaceGenerated(indian)
            ElseIf EthChoix == 6
                ETHtxt = "white"
                setRaceGenerated(white)
            EndIf
            DoorMan = generatePersonTemporary()
            While !DoorMan.isInterestedIn(Player)
                DoorMan = generatePersonTemporary()
            EndWhile
            If DoorMan.isEthnic(black) && DoorMan.isMale()
                DoorMan.blendPreset(strt_black_m)
            ElseIf DoorMan.isEthnic(black) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_black_f)
            ElseIf DoorMan.isEthnic(indian) && DoorMan.isMale()
                DoorMan.blendPreset(strt_indian_m)
            ElseIf DoorMan.isEthnic(indian) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_indian_f)
            ElseIf DoorMan.isEthnic(arab) && DoorMan.isMale()
                DoorMan.blendPreset(strt_arab_m)
            ElseIf DoorMan.isEthnic(arab) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_arab_f)
            ElseIf DoorMan.isEthnic(latin) && DoorMan.isMale()
                DoorMan.blendPreset(strt_latin_m)
            ElseIf DoorMan.isEthnic(latin) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_latin_f)
            ElseIf DoorMan.isEthnic(asian) && DoorMan.isMale()
                DoorMan.blendPreset(strt_asian_m)
            ElseIf DoorMan.isEthnic(asian) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_asian_f)
            ElseIf DoorMan.isEthnic(white) && DoorMan.isMale()
                DoorMan.blendPreset(strt_white_m)
            ElseIf DoorMan.isEthnic(white) && !DoorMan.isMale()
                DoorMan.blendPreset(strt_white_f)
            EndIf
            DoorMan.blendPreset(bodybuilder)
            DoorMan:age => random(25, 55)
            DoorMan:muscle => random(35, 75)
            DoorMan:fitness => random(15, 75)
            DoorMan:martial => random(5, 75)
            DoorMan.randomizeFace()
            DoorMan.randomizeHairs()
            DoorMan.dress()
            DoorMan.show()
            DoorMan.moveToPersonStand(Player, 50)
            faceEachOther(DoorMan, Player)
            perversion += 0.5
            DoorMan(Curious)::"Hi, what can I do for you, sir?"
            Player(Curious)::"I would like to join the exclusive swingers club!"
            If !DoorMan.isSameEthnic(Player)
                DoorMan(Sad)::"Sorry <Player.sir_or_miss>, we only accept <ETHtxt> people!"
                Player(Sad)::"Ok fine, thanks!"
            Else
                beinito = true
            EndIf
            
        ElseIf setESwingerClub >= 1 && Mot2 <= 0
            DoorMan.show()
            DoorMan(Serene)::"Hi <Player.sir_or_miss>, you must pay your registration to enter! According to our documents, you have not paid your month!"
            "What should I do?"
            0::"Pay <SCsetPricel>"
            1::"Pay later"
            
            If 0
                Player.setActorVar(rk_VB_SCsetDate, 0)
                Player.setActorVar(rk_VB_SCsetDateTemp, 0)
                "So I decided to pay my debt..."
                money -= SCsetPrice
                SCsetDate = Player.getActorVar(rk_VB_SCsetDate)
                SCsetDateT = Player.getActorVar(rk_VB_SCsetDateTemp)
                DoorMan(Happy)::"Thanks, you get go inside now! Have fun!"
                Player.setActorVar(rk_VB_SCsetDate, Mot)
                Player.setActorVar(rk_VB_SCsetDateTemp, Mot1)
                entre = true
            Else
                "Damn, I forgot about that!"
                Player(Sad)::"I'll come back later!"
                DoorMan(Happy)::"No problem, you're welcome to join us when you have the money!"
                DoorMan.hide()
            EndIf
        Else
            entre = true
        EndIf
        If beinito
            DoorMan(Happy)::"Yes, of course! So the price to join is <inscripl> per month."
            "What should I do?"
            0:: money > inscrip::"Subscribe"
            1::"Too expensive!"
            
            If 0
                perversion += 0.5
                money -= inscrip
                If EthChoix == 1
                    rk_VB_setESwingerClub.setGlobal(1)
                ElseIf EthChoix == 2
                    rk_VB_setESwingerClub.setGlobal(2)
                ElseIf EthChoix == 3
                    rk_VB_setESwingerClub.setGlobal(3)
                ElseIf EthChoix == 4
                    rk_VB_setESwingerClub.setGlobal(4)
                ElseIf EthChoix == 5
                    rk_VB_setESwingerClub.setGlobal(5)
                ElseIf EthChoix == 6
                    rk_VB_setESwingerClub.setGlobal(6)
                EndIf
                Player.setActorVar(rk_VB_SCsetDate, Mot)
                Player.setActorVar(rk_VB_SCsetDateTemp, Mot1)
                Player.setActorVar(rk_VB_SCsetPrice, inscrip)
                DoorMan(Happy)::"Congratulations, you are now an official member of our exclusive club! I wish you fun and adventures of all kinds..."
                Player(Happy)::"Cool thanks!"
                "Do I take advantage now of my registration to go and fuck or do I wait?"
                0::"Sure, let's go inside!"
                1::"I'll wait!"
                
                If 0
                    entre = true
                EndIf
            Else
                Player(Sad)::"You know what, I'll think about it again because I don't have the money right now!"
                DoorMan(Happy)::"No problem, you're welcome to join us when you have the money!"
                DoorMan.hide()
            EndIf
        EndIf
        
        If entre
            If Dating.isValid() || isWithCompanion()
                If Dating.isValid()
                    "I'm currently with <Dating.name> should I go inside with <Dating.him_or_her>?"
                ElseIf isWithCompanion()
                    "I'm currently with <CurrentCompanion.name> should I go inside with <CurrentCompanion.him_or_her>?"
                EndIf
                0::"Sure, let's go inside!"
                1::"Only me"
                99::"I'll wait!"
                
                If 0
                    rk_VB_SCsetDateGo.getGlobal(1)
                    entreSalle = true
                ElseIf 1
                    Player(Sad)::"Humm, sorry but I forget that I have important things to do! So see you later!"
                    If Dating.isValid()
                        Dating.show()
                        Dating(Curious)::"Fine see you later!"
                    ElseIf isWithCompanion()
                        CurrentCompanion.show()
                        CurrentCompanion(Curious)::"Fine see you later!"
                    EndIf
                    endDate()
                    entreSalle = true
                Else
                    "I decided to go somewhere else!"
                EndIf
            Else
                entreSalle = true
            EndIf
            If entreSalle
                perversion += random(1, 2)
                followUp(rk_VB_SCSwingerSalle)
            EndIf
        EndIf
    SceneEnd()