WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        
        RoomVisitChoice = rk_VB_RoomVisitChoice.getGlobal()
        ResetTrueA = false
        HotGotoR = false
        Hunger = Player.getActorVar(rk_VB_Hunger)
        HungerEnable = rk_VB_EnableHunger.getGlobal()
        UrgeToPeeEnable = rk_VB_UrgeToPeeEnable.getGlobal()
        UrgeToPee = Player.getActorVar(rk_VB_UrgeToPee)
        RentHotelRoom = rk_VB_RentHotelRoom.getGlobal()
        GetRoom = Player.getActorVar(rk_VB_TempGetADeals)
        HotelRentlastday = Player.getActorVar(rk_VB_setHotelRentlastday)
        tag_HLandlord = Landlord.getActorVar(rk_VB_tag_HLandlord)
        If isModEnabled(rk_vin_Base)
            MaPC = Player:age*365
            If HotelRentlastday <= 0
                HotelRentdayP = HotelRentlastday + MaPC
            Else
                HotelRentdayP = HotelRentlastday + 1
            EndIf
            DPMaPC = MaPC + 1
            If RentHotelRoom == 1 && DPMaPC >= HotelRentdayP
                ResetTrueA = true
            ElseIf RentHotelRoom == 0 && DPMaPC <= HotelRentdayP
                ResetTrueA = true
            EndIf
            If ResetTrueA            
                rk_VB_RentHotelRoom.setGlobal(0)
                Player.setActorVar(rk_VB_TempGetADeals, 0)
                Player.setActorVar(rk_VB_setHotelRentlastday, 0)
                Landlord.setActorVar(rk_VB_tag_HLandlord, 0)
                rk_VB_RoomVisitChoice.setGlobal(0)
            EndIf
            "I decided to go to my room...."
            If RentHotelRoom == 0
                "I didn't rent any room, or my room is not available anymore, I have to rent for another day..."
            Else
                HotGotoR = true
            Endif
            If HotGotoR
                continueStoryH = true
                If Random(0, 100) > 75
                    continueStoryH = false
                    followUp(rk_VB_Catch_maid_steals)
                Endif
                If continueStoryH
                    setBackground(hotel)
                    If RoomVisitChoice == 1
                        setBackground3D(Modules/vin_Base/Bedrooms/0.lpworld, true, true)
                    ElseIf RoomVisitChoice == 2
                        setBackground3D(Modules/vin_Base/Bedrooms/1.lpworld, true, true)
                    ElseIf RoomVisitChoice == 3
