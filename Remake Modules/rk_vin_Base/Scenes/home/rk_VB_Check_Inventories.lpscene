WHAT: none
WHERE: none
WHEN: 0 - 24
WHO: none
OTHER: none

    SceneStart()
        If isModEnabled(rk_vin_Base) || isModEnabled(rk_Home_Inventories)
            clearGetList()
            ETMaison = false
            If isAtHome() || PlayerHasHome() || isAtDatingHome()
                ETMaison = true
            EndIf
            hasUniRoom = rk_has_Fratroom.getGlobal()
            GUnRoom = rk_VB_setBackgroundRoom.getGlobal()
            hasLivingroom = RE_has_lvingroom.getGlobal()
            hasKitchen = RE_has_kitchen.getGlobal()
            hasOfficeroom = RE_has_officeroom.getGlobal()
            hasSecBedroom = RE_has_secondbedroom.getGlobal()
            hasThirdBedroom = RE_has_thirdbedroom.getGlobal()
            If !ETMaison
                If hasUniRoom == 0
                    "You need to find a personal home first!"
                Else
                    conti = true
                EndIf
            Else
                conti = true
            EndIf
            If conti
                "What do I want to verify?"
                0::isModEnabled(rk_vin_Base) || isModEnabled(rk_ShopAddons) || isModEnabled(rk_EroticShop)::"Check adult toy box"
                1::isModEnabled(rk_vin_Base) || isModEnabled(rk_ShopAddons) || isModEnabled(rk_SeaFoodShop) || isModEnabled(rk_IceCreamShop)::"Check freezer"
                2::"Check garage"
                3::isModEnabled(rk_vin_Base) || isModEnabled(rk_MoneyMaker) || isModEnabled(rk_ShopAddons) ||  isModEnabled(rk_JewelryShop)::"Check jewellery box"
                4::hasKitchen > 0::"Check kitchen"
                5::hasLivingroom > 0 ::"Check living room"
                12::"Check my bedroom"
                6::hasOfficeroom > 0::"Check office room"
                7::"Check pantry"
                8::"Check refrigerator"
                9::hasSecBedroom > 0::"Check second bedroom"
                10::"Check storage room"
                11::hasThirdBedroom > 0::"Check third bedroom"
                99::"I changed my mind"
            
                If 0
                    followUp(rk_VB_check_AdultToyBox)
                ElseIf 1
                    followUp(rk_VB_check_freezer)
                ElseIf 2
                    followUp(rk_VB_check_Garage)
                ElseIf 3
                    followUp(rk_VB_check_jewelbox)
                ElseIf 4
                    followUp(rk_VB_check_kitchen)
                ElseIf 5
                    followUp(rk_VB_check_livingroom)
                ElseIf 12
                    followUp(rk_VB_check_mydbedroom)
                ElseIf 6
                    followUp(rk_VB_check_officeroom)
                ElseIf 7
                    followUp(rk_VB_check_pantry)
                ElseIf 8
                    followUp(rk_VB_check_refrigerator)
                ElseIf 9
                    followUp(rk_VB_check_secondbedroom)
                ElseIf 10
                    followUp(rk_VB_check_storage_room)
                ElseIf 11
                    followUp(rk_VB_check_thirdbedroom)
                EndIf
            EndIf
        EndIf
    SceneEnd()