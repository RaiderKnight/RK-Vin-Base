WHAT: none
WHERE: none
WHEN: 0-24
WHO: none
OTHER: none

    SceneStart()
        lactation_IC = Player.getActorVar(rk_VB_lactation_IC_Don)
        Hunger = Player.getActorVar(rk_VB_Hunger)
        HungerEnable = rk_VB_EnableHunger.getGlobal()
        UrgeToPeeEnable = rk_VB_UrgeToPeeEnable.getGlobal()
        UrgeToPee = Player.getActorVar(rk_VB_UrgeToPee)
        Actor = generatePersonTemporary()
        GiveDonation = false
        If Player.isMale()
            While Actor.isMale()
                Actor = generatePersonTemporary()
            endWhile
        ElseIf Player.isTrans()
            While Actor.isTrans()
                Actor = generatePersonTemporary()
            endWhile
        ElseIf !Player.isMale()
            While !Actor.isMale()
                Actor = generatePersonTemporary()
            endWhile
        EndIf
        Actor:age => Random(18, 21)
        Actor.makeInterested(Player)
        Actor.dress()
        Actor.show()
        Actor(Happy)::"Hi, how can I help you?"
        Player(Curious)::"I am here to offer a donation!"
        Actor(Happy)::"Great, please follow me!"
        setBackground(brothel)
        If isModEnabled(rk_MoneyMaker)
            setBackground3D(Modules/rk_MoneyMaker/World/Massage.lpworld)
        Else
            setBackground3D(Modules/rk_vin_Base/World/Massage.lpworld)
        EndIf
        Actor(Happy)::"How many donations do you want to give?"
        turnCount = 0
        0::"1 donation"
        1::"5 donations"
        2::"10 donations"
        99::"I changed my mind!"
        
        If 0
            turnCount = 1
        ElseIf 1
            turnCount += 5
        ElseIf 2
            turnCount += 10
        Else
            sceneEnd()
        EndIf
        Actor(Flirty)::"I will be happy to help you if you wish?"
        0::"Yeah sure!"
        1::"I prefer to be alone!"
        
        If 1
            GoSolo = true
        EndIf
        Counter = 0
        If GoSolo
            While Counter < turnCount
                "I started to press my boobs to let the milk coming out..."
                Filter(Solo)
                sex(Player)
                Counter += 1
                arousal += 40
                If HungerEnable > 0
                    Player.modifyActorVar(rk_VB_Hunger, -1)
                EndIf
                FillInML = random(150, 250)
                Player.modifyActorVar(rk_VB_lactation_IC_Don, FillInML)
                "I just give <FillInML>ml in donation!"
            EndWhile
            GiveDonation = true
            Player.dress()
        Else
            While Counter < turnCount
                "<Actor.name> started to press my boobs to let the milk coming out..."
                sex(Actor, Player)
                Counter += 1
                arousal += 40
                If HungerEnable > 0
                    Player.modifyActorVar(rk_VB_Hunger, -2)
                EndIf
                FillInML = random(150, 250)
                Player.modifyActorVar(rk_VB_lactation_IC_Don, FillInML)
                "I just give <FillInML>ml in donation!"
            EndWhile
            GiveDonation = true
            Player.dress()
        EndIf
        
        If GiveDonation
            Actor(Happy)::"Thank you very much for your donation. Your donation will be added to our delicious Formula Milk recipes."
            Price = 25*turnCount
            Price_loc = Price.convertToLocalCurrency()
            Actor(Happy)::"Here is <Price_loc> for your help!"
            money += Price
        EndIf
        Actor.hide()
    SceneEnd() // Created by Raddeck