                                Player.modifyActorVar(stock_Lovebotz, -0.005)
                                mood += 2.35
                                energy -= 9
                                fitness += 0.005
                                attractiveness += 0.002
                                Player.strip()
                                
                                Sex(Player)
                                passTime(0.25, 0.5)
                                
                            Endif
                        
                        Endif

                        If stock_Lovebotz <= 0
                            Player.setActorVar(stock_Lovebotz, 0)
                            Player.modifyActorVar(rk_VB_IBuySextoy, -1)
                        EndIf
                    
                    Endif
                
                Endif

            Endif

        Endif
    SceneEnd()
